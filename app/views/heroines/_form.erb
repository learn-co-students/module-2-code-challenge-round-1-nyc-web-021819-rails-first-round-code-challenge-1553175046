<%= render "/layouts/errors" %>

<%= form_for @heroine do |f| %>
  <p>
  <%= f.label :name %>
  <%= f.text_field :name %>
  </p>
  <p>
  <%= f.label :super_name %>
  <%= f.text_field :super_name %>
  </p>
  <p>
  <%= f.label :power %>
  <%= f.collection_select(:power_id, @powers, :id, :name) %>
  </p>
  <p>
  <%= f.submit %>
  </p>
<% end %>
